<Page x:Class="WpfApp1.Pages.BuyerMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="BuyerMenu">

    <Grid Background="White">
        <Button x:Name="Users" Content="Пользователи" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="48" Click="Users_Click"/>
        <Button x:Name="Employees" Content="Сотрудники" HorizontalAlignment="Left" Margin="167,10,0,0" VerticalAlignment="Top" Height="48"/>
        <Frame x:Name="BuyerFrame"/>
    </Grid>
</Page>
